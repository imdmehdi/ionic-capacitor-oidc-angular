{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "ionic-capacitor-oidc-angular": {
      "root": "libs/ionic-capacitor-oidc-angular",
      "sourceRoot": "libs/ionic-capacitor-oidc-angular/src",
      "projectType": "library",
      "prefix": "libs/ionic-capacitor-oidc-angular",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/ionic-capacitor-oidc-angular/tsconfig.lib.json",
            "project": "libs/ionic-capacitor-oidc-angular/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "libs/ionic-capacitor-oidc-angular/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/ionic-capacitor-oidc-angular/src/test.ts",
            "tsConfig": "libs/ionic-capacitor-oidc-angular/tsconfig.spec.json",
            "karmaConfig": "libs/ionic-capacitor-oidc-angular/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ionic-capacitor-oidc-angular/tsconfig.lib.json",
              "libs/ionic-capacitor-oidc-angular/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "libs": {
      "root": "libs",
      "sourceRoot": "libs",
      "projectType": "library",
      "prefix": "foo",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "testing/test.libs.ts",
            "tsConfig": "testing/tsconfig.libs.spec.json",
            "karmaConfig": "testing/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "testing/tsconfig.libs.json",
              "testing/tsconfig.libs.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "xplat": {
      "root": "xplat",
      "sourceRoot": "xplat",
      "projectType": "library",
      "prefix": "foo",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "testing/test.xplat.ts",
            "tsConfig": "testing/tsconfig.xplat.spec.json",
            "karmaConfig": "testing/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "testing/tsconfig.xplat.json",
              "testing/tsconfig.xplat.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ionic-sample": {
      "root": "apps/ionic-sample/",
      "sourceRoot": "apps/ionic-sample/src",
      "projectType": "application",
      "prefix": "foo",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nstudio/schematics",
    "packageManager": "yarn"
  },
  "defaultProject": "ionic-capacitor-oidc-angular"
}