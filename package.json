    "start.ionic.sample": "cd apps/ionic-sample && npm start",
    "clean.ionic.sample": "cd apps/ionic-sample && npx rimraf -- hooks node_modules platforms www plugins ios android package-lock.json && npm i && rimraf -- package-lock.json"
